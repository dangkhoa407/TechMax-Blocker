{
    "manifest_version": 3,
    "name": "TechMax Blocker",
    "version": "1.2",
    "description": "Hạn chế tiếp xúc các trang web độc hại bằng công nghệ AI",
    "permissions": ["tabs", "activeTab", "storage"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
     
    },
    
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
    "web_accessible_resources": [
        {
          "resources": ["blocked.html"],
          "matches": ["<all_urls>"]
        }
      ]
  }
  